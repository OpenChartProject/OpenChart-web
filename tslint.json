{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "forin": false,
    "no-console": false,
    "no-unused-variable": [
      true
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always"
      }
    ],
    "ordered-imports": [
      true,
      {
        "grouped-imports": true
      }
    ]
  }
}
